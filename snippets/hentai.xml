  <project path="vendor/qcom/opensource/power" name="platform_vendor_qcom_opensource_power" groups="vendor" revision="queenslave" remote="github/hentaiOS" />
  <project path="vendor/qcom/opensource/thermal-engine" name="platform_vendor_qcom_opensource_thermal-engine" revision="queenslave" remote="github/hentaiOS" />

